<!DOCTYPE module PUBLIC "//gwt-module/" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.2/distro-source/core/src/gwt-module.dtd">
<module>

      <!-- Inherit the core Web Toolkit stuff.                        -->
      <inherits name='com.google.gwt.user.User'/>
      
      <inherits name="com.google.gwt.inject.Inject"/>
      
      <inherits name="us.gibb.dev.gwt.Patterns"/>

      <!-- Adapter for Hibernate/JPA-->
      <inherits name='net.sf.gilead.Adapter4Gwt15'/>
      <inherits name='net.sf.gilead.emul.java5.ejb3.Ejb3'/>
 
      <!--  inherit css based theme -->
      <inherits name='com.google.gwt.user.theme.chrome.Chrome'/>
      
      <!-- Specify the app entry point class.                         -->
      <entry-point class='us.gibb.dev.gwt.demo.client.Application'/>

      <!-- Specify the application specific style sheet.              -->
      <stylesheet src='Application.css' />
      
      <source path="client" />
      <source path="model" />
       
      <!-- <servlet path='/us.gibb.dev.gwt.demo.Application/service.gwtrpc' class='org.springbyexample.web.gwt.server.ServiceImpl'/> -->

</module>
